{
  "name": "langchain-rag",
  "version": "1.0.0",
  "main": "dist/server.js",
  "repository": "https://github.com/plake492/langchain-rag.git",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "qdrant": "docker run -p 6333:6333 qdrant/qdrant",
    "scrape": "ts-node -r tsconfig-paths/register src/scripts/scrape-sources.ts",
    "test:embeddings": "ts-node -r tsconfig-paths/register src/scripts/test-embeddings.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix"
  },
  "dependencies": {
    "@clickhouse/client": "^1.13.0",
    "@langchain/classic": "^1.0.3",
    "@langchain/community": "^1.0.2",
    "@langchain/core": "^1.0.5",
    "@langchain/openai": "^1.1.1",
    "@langchain/qdrant": "^1.0.0",
    "@langchain/textsplitters": "^1.0.0",
    "@qdrant/js-client-rest": "^1.11.0",
    "axios": "^1.7.7",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "langchain": "^1.0.4",
    "morgan": "^1.10.0",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.5",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.7",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  }
}
